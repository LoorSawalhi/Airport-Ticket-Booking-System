flight table => 
	id
	depature date
	depature airport id
	arrival airport id

flight_class_relation =>
	flight id
	class id
	price
	
	
class =>
	id
	price	
	min price
	max price
	max seat
	
airport =>
	id
	airport name
	country
	

booking =>
	flight id
	class id
	passenger id
	
passenger =>
	id
	name


